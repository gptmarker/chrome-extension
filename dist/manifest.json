{
    "name": "GPTMarker",
    "author": "Altogic",
    "description": "ChatGPT - Share and save your ChatGTP conversations",
    "version": "1.0.2",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://chat.openai.com/*"],
            "js": ["js/content.js"]
        }
    ],
    "permissions": ["activeTab", "tabs", "storage", "scripting"],
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "64": "images/64.png",
        "128": "images/128.png",
        "256": "images/256.png"
    }
}
