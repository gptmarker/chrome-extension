{
    "name": "GPTMarker",
    "author": "Altogic",
    "description": "GPTMarker - Share and save your ChatGPT conversations",
    "version": "1.0.6",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://chat.openai.com/chat"],
            "js": ["js/content.js"],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "64": "images/64.png",
        "128": "images/128.png",
        "256": "images/256.png"
    }
}
